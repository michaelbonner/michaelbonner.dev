{
	"name": "michaelbonner-svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint . && prettier --check .",
		"format": "prettier --write .",
		"out": "bun outdated",
		"up": "bun update",
		"preinstall": "npx only-allow bun",
		"test:e2e": "playwright test",
		"test:setup": "bun ./e2e/validate-test-data.js --update",
		"test": "bun test:setup && bun test:e2e"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.1",
		"@eslint/js": "^9.39.2",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/enhanced-img": "0.8.5",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@typescript-eslint/eslint-plugin": "^8.53.1",
		"@typescript-eslint/parser": "^8.53.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.0.0",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"sharp": "^0.34.5",
		"svelte": "^5.48.0",
		"svelte-check": "^4.3.5",
		"svelte-highlight": "^7.9.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/newsreader": "^5.2.10",
		"@qwik.dev/partytown": "^0.11.2",
		"clsx": "^2.1.1",
		"photoswipe": "^5.4.4",
		"posthog-js": "^1.334.1",
		"posthog-node": "^5.24.1"
	}
